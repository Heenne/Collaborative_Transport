<launch>
    <include file="$(find transport_launcher)/launch/virtual_master.launch">
        <arg name="use_ps4_cmd_vel"  value="true"/>
        <arg name="use_planner_cmd_vel"  value="false"/>
    </include>
     <node name="transport_state_machine" type="transport_state_machine_node.py" pkg="transport_controller" output="screen">
        <rosparam param="base_namespaces">["/mur/mir","/miranda/mir"]</rosparam>
        <rosparam param="arm_namespaces">["/mur/mir","/miranda/panda"]</rosparam>
        <param name="enable_manipulator" value="true"/>
        <param name="enable_orientation_ff" value="false"/>
    </node>
</launch>